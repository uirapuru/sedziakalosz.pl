{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Movie</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ entity.url }}</td>
            </tr>
            <tr>
                <th>Autorid</th>
                <td>{{ entity.autorId }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ entity.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Analysis</th>
                <td>{{ entity.analysis }}</td>
            </tr>
            <tr>
                <th>Judgename</th>
                <td>{{ entity.judgeName }}</td>
            </tr>
            <tr>
                <th>Eventdescription</th>
                <td>{{ entity.eventDescription }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('movie') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('movie_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
